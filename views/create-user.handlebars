<h1 id="joinus">Create Your Account</h1>
<div class="container" id="login">
    <form class="create-form">
        <div class="form-group">
            <div id="firstname-problem" class="problem"></div>
                <label for="first-name"><i class=""></i></label>
                <input type="text" id="first-name" name="first-name" placeholder="First Name">
        </div>
    <hr>
        <div class="form-group">
            <div id="lastname-problem" class="problem"></div>
                <label for="last-name"><i class=""></i></label>
                <input type="text" id="last-name" name="last-name" placeholder="Last Name">
        </div>
    <hr>
        <div class="form-group">
            <div id="email-problem" class="problem"></div>
                <label for="email"><i class=""></i></label>
                <input type="text" id="email" name="email" placeholder="E-mail">
        </div>
    <hr>
        <div class="form-group">
            <div id="birthdate-problem" class="problem"></div>
                <label for="birthdate"><i class=""></i></label>
                <input type="date" id="birthdate" name="birthdate" placeholder="1990-05-15">
        </div>
    <hr>
        <div class="form-group">
            <div id="phone-problem" class="problem"></div>
                <label for="phone"><i class=""></i></label>
                <input type="tel" id="phone" name="phone" placeholder="555-666-1234">
        </div>
    <hr>
        <div id="city-problem" class="problem"></div>
            <div class="form-group">
                <label for="city"><i class=""></i></label>
                <input type="text" id="city" name="city" placeholder="City">
        </div>
    <hr>
        <div class="form-group">
            <div id="state-problem" class="problem"></div>
                <label for="state"></label>
                <select class="form-control" id="state" value="AK">
                <option>AK</option>
                <option>AL</option>
                <option>AR</option>
                <option>AZ</option>
                <option>CA</option>
                <option>CO</option>
                <option>CT</option>
                <option>DC</option>
                <option>DE</option>
                <option>FL</option>
                <option>GA</option>
                <option>HI</option>
                <option>IA</option>
                <option>ID</option>
                <option>IL</option>
                <option>IN</option>
                <option>KS</option>
                <option>KY</option>
                <option>LA</option>
                <option>MA</option>
                <option>MD</option>
                <option>ME</option>
                <option>MI</option>
                <option>MN</option>
                <option>MO</option>
                <option>MS</option>
                <option>MT</option>
                <option>NC</option>
                <option>ND</option>
                <option>NE</option>
                <option>NH</option>
                <option>NJ</option>
                <option>NM</option>
                <option>NV</option>
                <option>NY</option>
                <option>OH</option>
                <option>OK</option>
                <option>OR</option>
                <option>PA</option>
                <option>RI</option>
                <option>SC</option>
                <option>SD</option>
                <option>TN</option>
                <option>TX</option>
                <option>UT</option>
                <option>VA</option>
                <option>VT</option>
                <option>WA</option>
                <option>WI</option>
                <option>WV</option>
                <option>WY</option>
                </select>
        </div>
    <hr>
        <div class="form-group">
            <div id="zip-problem" class="problem" class="problem"></div>
                <label for="zipcode"><i class=""></i></label>
                <input type="number" id="zipcode" name="zipcode" placeholder="97122">
        </div> 
    <hr>
        <div id="double-house-problem" class="problem"></div>
            <div class="row">
                <div class="col-sm-6">
                    <h2>Already have a House?</h2>
                    <div class="form-group">
                        <div id="house-problem" class="problem"></div>
                        <label for="house"><i class=""></i></label>
                        <input type="number" id="house" name="house" placeholder="House ID #">
                    </div>
                    <div class="form-group">
                        <label for="house-pass"><i class=""></i></label>
                        <input type="password" id="house-pass" name="house-pass" placeholder="House Password">
                    </div>
                </div>
                <div class="col-sm-6">
                    <h2>Create New House</h2>
                    <div class="form-group">
                        <div id="new-house-problem" class="problem"></div>                    
                        <label for="new-house"><i class=""></i></label>
                        <input type="number" id="new-house" name="new-house" placeholder="House ID #">
                    </div>
                    <div class="form-group">
                        <label for="new-house-pass"><i class=""></i></label>
                        <input type="password" id="new-house-pass" name="new-house-pass" placeholder="Create Password">
                    </div>
                    <div class="form-group">
                        <label for="new-house-pass-confirm"><i class=""></i></label>
                        <input type="password" id="new-house-pass-confirm" name="new-house-pass-confirm" placeholder="Confirm Password">
                    </div>
                </div>
                {{!-- <div class="col-sm-6"></div> --}}
            </div>
        <button type="submit" id="orderButton">Submit</button>
    </form>
</div>   

